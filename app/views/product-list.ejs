    <table class="ts sortable padded large table">
        <thead>
            <tr>
                <% for(var i=0; i< cols.length; i++) { %>
                    <th>
                        <%= cols[i] %>
                    </th>
                    <% } %>
            </tr>
        </thead>
        <tbody>
            <% for(var i=0; i< data.length; i++) { %>
                <tr>
                    <td>
                        <button class="ts primary button">
          <i class="heart icon"></i>
          <%= data[i].PName %>
        </button>
                    </td>
                    <td>
                        <%= data[i].Classification.CName %>
                    </td>
                    <td>
                        <%= data[i].PSpecification %>
                    </td>
                    <td>
                        <%= data[i].PUnit %>
                    </td>
                </tr>
                <% } %>
        </tbody>
        <tfoot>
            <tr>
                <th colspan="5">
                    <%= title %>
                </th>
            </tr>
        </tfoot>
    </table>

